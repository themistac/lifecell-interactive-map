    // Setup the different icons and shadows
    var iconURLPrefix = '/images/lifecell/';
    
    var icons = [
      iconURLPrefix + 'doctor-icon.png',
      iconURLPrefix + 'doctor-icon.png',
      iconURLPrefix + 'doctor-icon.png',
      iconURLPrefix + 'hospital-icon.png',
      iconURLPrefix + 'doctor-icon.png',
